    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "pg": "^8.2.1",
    "razorpay": "^2.0.6",
    "sequelize": "^5.21.11",
    "shortid": "^2.2.15",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^8.10.1",
    "tsc": "^1.20150623.0",
    "typescript": "3.5.1",
    "winston": "^3.2.1"
