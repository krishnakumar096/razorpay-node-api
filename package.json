  },
  "homepage": "https://gitlab.com/simpragma/starters/TypeScript-Node-Starter#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bcryptjs": "2.4.2",
    "@types/chai": "^4.2.11",
    "@types/jwt-simple": "0.5.33",
    "@types/mocha": "^7.0.2",
    "@types/node": "10.12.12",
    "@types/passport": "0.4.7",
    "@types/passport-jwt": "3.0.1",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
